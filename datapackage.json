  "views": [
    {
      "name": "total-users-and-users-on-pages-with-premium-offer",
      "title": "Total users and users on pages with premium offer",
      "resources": ["premium-data-funnel-overall-weekly"],
      "specType": "simple",
      "spec": {
        "type": "line",
        "group": "date",
        "series": [
          "total users",
          "users on pages with premium offer"
        ]
      }
    },
    {
      "name": "kpi-1",
      "title": "Visitors of pages with premium offer / total visitors",
      "resources": [
        {
          "name": "premium-data-funnel-overall-weekly",
          "transform": [
            {
              "type": "formula",
              "expressions": [
                "data['users on pages with premium offer'] * 100 / data['total users']"
              ],
              "asFields": [
                "users on pages with premium offer ratio"
              ]
            }
          ]
        }
      ],
      "specType": "simple",
      "spec": {
        "type": "line",
        "group": "date",
        "series": [
          "users on pages with premium offer ratio"
        ],
        "ySuffix": "%"
      }
    },
    {
      "name": "kpi-2",
      "title": "Clicks on the offer / visitors of pages with premium offer",
      "resources": [
        {
          "name": "premium-data-funnel-overall-weekly",
          "transform": [
            {
              "type": "formula",
              "expressions": [
                "data['total clicks'] * 100 / data['users on pages with premium offer']",
                "data['clicks on core data'] * 100 / data['users on core datasets']",
                "data['clicks on sports data'] * 100 / data['users on sports data']"
              ],
              "asFields": [
                "Total clicks on the offer",
                "Clicks on core data pages",
                "Clicks on sports data pages"
              ]
            },
            {
              "type": "formula",
              "expressions": [
                "data['clicks on machine learning'] * 100 / data['users on machine learning data']",
                "data['clicks on cryptocurrency data'] * 100 / data['users on crypto data']",
                "data['clicks on s-and-p-500-financial-data'] * 100 / data['users on s&p500 datasets']"
              ],
              "asFields": [
                "Clicks on machine learning pages",
                "Clicks on crypto pages",
                "Clicks on S&P500 pages"
              ]
            },
            {
              "type": "formula",
              "expressions": [
                "data['clicks on world-bank data'] * 100 / data['users on world bank datasets']",
                "data['clicks on unece data'] * 100 / data['users on unece datasets']",
                "data['clicks on search page'] * 100 / data['users on search page']"
              ],
              "asFields": [
                "Clicks on world bank pages",
                "Clicks on UNECE pages",
                "Clicks on search page"
              ]
            },
            {
              "type": "formula",
              "expressions": [
                "data['clicks on collections pages'] * 100 / data['users on collections pages']",
                "data['clicks on dashboard page'] * 100 / data['users on dashboard page']",
                "data['clicks on pricing page'] * 100 / data['users on pricing page']"
              ],
              "asFields": [
                "Clicks on collections pages",
                "Clicks on dashboard page",
                "Clicks on pricing page"
              ]
            }
          ]
        }
      ],
      "specType": "simple",
      "spec": {
        "type": "line",
        "group": "date",
        "series": [
          "Total clicks on the offer",
          "Clicks on core data pages",
          "Clicks on sports data pages",
          "Clicks on machine learning pages",
          "Clicks on crypto pages",
          "Clicks on S&P500 pages",
          "Clicks on world bank pages",
          "Clicks on UNECE pages",
          "Clicks on search page",
          "Clicks on collections pages",
          "Clicks on dashboard page",
          "Clicks on pricing page"
        ],
        "ySuffix": "%"
      }
    },
    {
      "name": "kpi-3",
      "title": "Form submissions / clicks on the offer",
      "resources": [
        {
          "name": "premium-data-funnel-overall-weekly",
          "transform": [
            {
              "type": "formula",
              "expressions": [
                "data['total submissions'] * 100 / data['total clicks']"
              ],
              "asFields": [
                "form submission ratio"
              ]
            }
          ]
        }
      ],
      "specType": "simple",
      "spec": {
        "type": "line",
        "group": "date",
        "series": [
          "form submission ratio"
        ],
        "ySuffix": "%"
      }
    },
